<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- 监听实现 -->
	<bean id="rocketMqListener" class="com.jw.hms.framework.m3013.f001.mq.rocketmq.RocketMqConsumerService"></bean>
	<!-- 监听配置 -->
	<bean id="consumer" class="com.jw.hms.framework.m3013.f001.mq.rocketmq.RocketMqConsumer" init-method="init">
		<property name="consumerGroup" value="PushConsumer" />
		<property name="namesrvAddr" value="rocketmq1:9876;rocketmq2:9876" />
		<property name="topic" value="myTopic" />
		<property name="subExpression" value="myTopic-tags" />
		<property name="rocketMqConsumerService" ref="rocketMqListener" />
	</bean>

</beans>