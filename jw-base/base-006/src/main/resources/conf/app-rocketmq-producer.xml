<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- producer配置 -->
    <bean id="rocketmqImpl" class="com.jw.base.framework.core.mq.rocketmq.producer.RocketMqProducerServiceImpl" init-method="init">
        <property name="producerGroup" value="PushProducer"/>
        <property name="namesrvAddr" value="rocketmq1:9876;rocketmq2:9876" />
        <property name="topic" value="defaultTopic"/>
        <property name="keys" value="defaultKeys"/>
        <property name="tags" value="defaultTags"/>
    </bean>

</beans>  